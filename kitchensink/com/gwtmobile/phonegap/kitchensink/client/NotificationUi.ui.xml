<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:com.gwtmobile.ui.client.widgets">
	<ui:style>
	</ui:style>
	<g:HTMLPanel>
		<m:HeaderPanel caption="Notification" leftButton="Back"></m:HeaderPanel>
	
		<g:Button ui:field="alert">Alert</g:Button>
		<g:Button ui:field="confirm">Confirm</g:Button>
		<g:Button ui:field="beep">Beep</g:Button>
		<g:Button ui:field="vibrate">Vibrate</g:Button>
		<g:TextBox ui:field="text"></g:TextBox>
		<g:Button ui:field="blink">Blink</g:Button>
		<g:Button ui:field="activity">Activity</g:Button>
		<g:Button ui:field="progress">Progress</g:Button>
	</g:HTMLPanel>
</ui:UiBinder> 