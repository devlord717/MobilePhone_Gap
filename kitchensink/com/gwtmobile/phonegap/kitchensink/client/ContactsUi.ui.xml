<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:com.gwtmobile.ui.client.widgets">
	<ui:style>
		table > tbody > tr:last-child {
			height: 50%;
		}
	</ui:style>
	<g:HTMLPanel>
		<m:HeaderPanel caption="Contacts" leftButton="Back"></m:HeaderPanel>
		<m:Scroller>
			<g:HTMLPanel styleName="Container">
				<m:ListPanel ui:field="list">
					<g:Label>Create New Contact</g:Label>
					<g:Label>Find Contacts</g:Label>
					<g:Label>Clone Contact</g:Label>
					<g:Label>Delete Contact</g:Label>
				</m:ListPanel>
				<g:Label>PhoneGap contact creation function is not working as of version 0.9.3. Only find function is tested at this point.</g:Label>
				<g:HTML ui:field="text" styleName="Output">Console</g:HTML>
			</g:HTMLPanel>
		</m:Scroller>
	</g:HTMLPanel>
</ui:UiBinder> 