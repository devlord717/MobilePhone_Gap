<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:com.gwtmobile.ui.client.widgets"
	xmlns:p="urn:import:com.gwtmobile.phonegap.kitchensink.client">
	<ui:style>
		.container {
			padding-top: .3em;
		}
	</ui:style>
	<g:HTMLPanel>
		<p:PhoneGapHeaderPanel caption="Camera"></p:PhoneGapHeaderPanel>
		<g:HTMLPanel styleName="Container {style.container}">
			<m:ListPanel ui:field="list">
				<g:Label>Get Picture</g:Label>
			</m:ListPanel>
			<m:ScrollPanel secondaryStyle="Output">
				<g:HTML ui:field="text">Console</g:HTML>
			</m:ScrollPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 