<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.4/distro-source/core/src/gwt-module.dtd">
<module rename-to='GwtMobilePhoneGapKitchenSink'>
	<inherits name="com.google.gwt.user.User" />

	<!-- Other module inherits                                      -->
	<inherits
		name='com.touchonmobile.gwtmobile.phonegap.gwtmobile_phonegap' />

	<source path="client" />

	<entry-point
		class="com.touchonmobile.gwtmobile.phonegap.kitchensink.client.KitchenSink">
	</entry-point>
	
	<set-property name="user.agent" value="safari"/>

</module>